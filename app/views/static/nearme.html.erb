<div class="container">
  <p>
  <div class="row title">
      <h1>Restaurants nearby</h1>
  </div>
  <% @google["results"].map do |restaurant| %>
  <div class="row">
    <div class="col-md-6">
      <h3><%= restaurant["name"]%></h3>
      <p><%= restaurant["vicinity"]%></p>
      <p><% restaurant["geometry"]["location"]["lat"] %></p>
      <p><% restaurant["geometry"]["location"]["lng"] %></p>
    </div>
    <div class="col-md-6">
    </div>
  </div>
  <% end %>
</div>
